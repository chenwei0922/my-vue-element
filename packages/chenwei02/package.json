{
  "name": "@chenwei02/qy-element",
  "version": "1.0.2",
  "main": "dist/lib/index.js",
  "module": "dist/es/index.mjs",
  "typings": "dist/lib/index.d.ts",
  "types": "dist/es/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/lib/index.js"
    },
    "./es": "./dist/es/index.mjs",
    "./lib": "./dist/lib/index.js",
    "./es/*.mjs": "./dist/es/*.mjs",
    "./es/*": "./dist/es/*.mjs",
    "./lib/*.js": "./dist/lib/*.js",
    "./lib/*": "./dist/lib/*.js",
    "./*": "./dist/*"
  },
  "unpkg": "dist/dist/index.full.js",
  "jsdelivr": "dist/dist/index.full.js",
  "style": "dist/dist/index.css",
  "files": [
    "es",
    "lib",
    "dist",
    "theme",
    "_virtual",
    "global.d.ts"
  ],
  "keywords": [
    "chenwei02",
    "vue3组件库"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "release-it",
    "build": "vite build",
    "dev": "gulp watchModules -f gulpfile.ts",
    "start": "gulp -f gulpfile.ts",
    "clean": "rm -rf dist/"
  },
  "author": "陈十一",
  "description": "Vue3组件库",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chenwei0922/qy-element.git"
  },
  "bugs": {
    "url": "https://github.com/chenwei0922/qy-element/issues"
  },
  "homepage": "https://chenwei0922.github.io/qy-element/",
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "vite-plugin-svg-icons": "^2.0.1"
  }
}
